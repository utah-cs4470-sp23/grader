read image "a.png" to a[H, W]
let v1 = array[j : W] to_int(a[0, j]{1} * 256.0)
let v2 = array[j : W] to_int(a[1, j]{1} * 256.0)
let lvc = array[i : W, j : W, k : W] \
    ((if i < j then 1 else 0) + \
     (if i < k then 1 else 0) + \
     (if j < k then 1 else 0)) % 2
// Main computation
show array[i : W] sum[k : W, j : W] lvc[i, j, k] * v1[j] * v2[k]
